{
    "name": "restyphp/loggy",
    "description": "Logger simple y liviano, PSR-3 compatible y agnostico a cualquier Framework",
    "version": "2.0.1",
    "keywords": ["logger", "log", "logging", "psr-3", "loggy"],
    "homepage": "https://github.com/mostofreddy/logger",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Federico Lozada Mosto",
            "email": "mosto.federico@gmail.com",
            "homepage": "http://www.mostofreddy.com.ar",
            "role": "Lead"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.*",
        "squizlabs/php_codesniffer": "3.0.*",
        "satooshi/php-coveralls": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Resty\\Loggy\\": "src/",
            "Resty\\Loggy\\Tests\\": "tests/src/"
        }
    }
